@{
    ViewBag.Title = "Home Page";
}

<!-- Start top Banner -->
<div class="banner">
    <div id="myCarousel" class="carousel slide" data-bs-ride="carousel">
        <ol class="carousel-indicators">
            <li data-bs-target="#myCarousel" data-bs-slide-to="0" class="active"></li>
            <li data-bs-target="#myCarousel" data-bs-slide-to="1"></li>
            <li data-bs-target="#myCarousel" data-bs-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="~/Content/images/banner/1596527312.png" />
            </div>
            <div class="carousel-item">
                <img src="~/Content/images/banner/1596478299.png" />
            </div>
            <div class="carousel-item">
                <img src="~/Content/images/banner/1596477739.png" />
            </div>
        </div>
        <a class="carousel-control-prev" href="#myCarousel" role="button" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </a>
        <a class="carousel-control-next" href="#myCarousel" role="button" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </a>
    </div>
</div>
<!-- End top Banner -->
<!-- Start Report Online -->
<div class="reportOnline">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-7 text-center">
                <div class="text">Check Your Reports Online >></div>
            </div>
            <div class="col-md-5 lightbg text-center">
                <button class="btn btn-danger">Download Report <i class="bi bi-download"></i></button>
