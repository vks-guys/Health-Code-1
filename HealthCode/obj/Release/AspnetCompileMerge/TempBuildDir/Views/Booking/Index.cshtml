@model IEnumerable<HealthCode.Bussiness.Entity.CartItemUser>
@{
    ViewBag.Title = "Booking";
}
<div class="container">
    <h4>Booking List</h4>
    <div class="row">
        <div id="CartItemDiv" class="col-md-8">
            <table class="table table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Mobile</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model.Count() > 0)
                    {
                        foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.Name</td>
                                <td>@item.Email</td>
                                <td>@item.Mobile</td>
                                <td><a href="#" onclick="fn_GetTestOrPackage(@item.Id)">Get</a></td>
                            </tr>
                        }
                    }
                    else
                    {
                        <tr>
                            <td colspan="5">No Records were found!</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
        <div id="CartItemDivTestOrPackage" class="col-md-4">

        </div>
    </div>
</div>

<script>
    function fn_GetTestOrPackage(Id) {
        $.ajax({
            url: '/Booking/CartTestOrPackageData',
            contentType: 'application/html; charset=utf-8',
            data: { "Id": Id },
            type: 'GET',
            dataType: 'html',
            success: function (result) {
                $('#CartItemDivTestOrPackage').html(result);
            },
            error: function (xhr, status) {
                alert(status);
            }
        })
    }
</script>